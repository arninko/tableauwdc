<root>
  <stragety>
    <maturity>
      <candidates>
        <candidate>
        </candidate>
        <candidate>
        </candidate>
        <candidate>
        </candidate>
        <candidate>
        </candidate>
        <candidate>
        </candidate>
      </candidates>
    </maturity>
    <selection>
      <candidate>
        <timing></timing>
        <finance>
          <CAPEX></CAPEX>
          <OPEX></OPEX>
          <ROI></ROI>
        </finance>
        <value></value>
      </candidate>
    </selection>
    <planning>
      <requirements>
        <control>
          <risk assessment="HAZOP, FMEA, SWOT, PETSLE">
            <functional>
              <value type="function" criticalicity="must">call forwarding</value>
              <value type="outlook" criticalicity="must">style</value>
            </functional>
            <infrastructures>        
              <infrastructure>        
              </infrastructure>
              <infrastructure>        
              </infrastructure>
            </infrastructures>
            <marketing>
              <stakeholders>
                <sponsors>
                  <sponsor relation="-2=strongly refuse, 0= neutral, 2=strongly supports" influence="1=negligable, 2= significant, 3=primary"></sponsor>
                </sponsors>
                <pm relation="" influence=""></pm>
                <internals>
                  <internal relation="" influence=""></internal>
                </internals>
                <consultancy></consultancy>
                <providers>
                  <providers relation="" influence=""></providers>
                </providers>
              </stakeholders>
              <stakeholderMap type="decartes, venn">
              </stakeholderMap>  
            </marketing>      
            <security></security>      
            <finance>
              <CAPEX></CAPEX>
              <OPEX></OPEX>
              <ROI></ROI>
            </finance>      
          </risk>
          <plan>
            <steps>
                <step style="realArrowOnGraph, virtualArrowOnGraph">
                  <value id=""></value>
                  <duration>
                    <planned>
                      <ownDuration></ownDuration>
                    </planned>
                    <actual></actual>
                  </duration>
                  <dependencies style="pointOnGraph">
                    <steps>
                      <step type="neededForNextStep, sequential, overlap, waiting, recurring"></step>
                    </steps>
                    <functional type="missingPrerequisite"></functional>
                    <complexityScale></complexityScale>
                    <startEarliest>maxPreviousFinishEarliest</startEarliest>
                    <finishEarliest>ownDuration + startEarliest</finishEarliest>
                    
                    <lastfinishLatest>lastfinishEarliest</lastfinishLatest>  
                    <startLatest>finishLatest - ownDuration</startLatest>
                    <finishLatest>nextStartLatest</finishLatest>  
                    <slacks>
                      <full aim="change duration without effect on critical path">finishLatest-startEarliest</full>
                      <independent aim="use this slack first and recalculate start/finish">finishEarliest-startLatest</independent>
                      <free aim="can be used after independent">finishEarliest-startEarliest</free>
                      <dependent aim="">finishLatest-startLatest</dependent>
                    </slacks>
                  </dependencies>
                  <resource>
                    <finance>
                      <planned estimationMethod="basedOnSimilarProject, basedOnUnitPrice, basedOnCostType, basedOnStepResourceMatrix">3 FTE only</planned>
                      <actual></actual>
                    </finance>
                    <equipment>
                      <planned>not needed</planned>
                      <actual></actual>
                    </equipment>
                    <people>
                      <planned>3 FTE</planned>
                      <actual></actual>
                    </people>
                  </resource>
                </step>
              <criticalPathMethod>
                <steps>
                  <step isCritical="Yes">0 = slack = startLatest - startEarliest = finishLatest - finishEarliest
                  </step>
                </steps>
              </criticalPathMethod>
              <estimation>
                <deviation>
                  <overdue>plannedCostOfPlanned(BCWS), plannedCostOfCompleted(BCWP) , target(SPI)=BCWP/BCWS, SPI LT 1 = overdue completion</overdue>
                  <overcost>plannedCostOfCompleted(BCWP), actualCostOfCompleted(ACWP), cost(CPI)=BCWP/ACWP, CPI LT 1 = overcost </overcost>
                </deviation>
                <completionTime>currentTimePoint(ATE), projectDuration(OD), completionTime(ETC)=ATE+(OD-ATE*SPI)/SPI</completionTime>
                <completionCost>projectCost(BAC), completionCost(EAC)=ACWP+(BAC-BCWP)/CPI</completionCost>
              </estimation>
            </steps>
          </plan>
        </control>
      </requirements>
      <applicationProcess type="open, selective, twolevel, invitation">
        <providers>
          <provider>
            <risk assessment="HAZOP, FMEA, SWOT, PETSLE">
                <responsibility>
                  <ownership type="onPromise, IaaS, PaaS, Saas"></ownership>
                </responsibility>      
                <security></security>      
                <bcpdrp></bcpdrp>
            </risk>
            <validation>
              <plan>
              </plan>
            </validation>
            <contracts></contracts>
            <finance>
              <CAPEX></CAPEX>
              <OPEX></OPEX>
              <COGS></COGS>
            </finance>
          </provider>
        </providers>
      </applicationProcess>
    </planning>
    <decision>
      <requirements>
          <risk assessment="HAZOP, FMEA, SWOT, PETSLE">
            <functional></functional>
            <system></system>
            <stakeholder>
              <sponsors></sponsors>
              <pm></pm>
              <internal></internal>
              <consultancy></consultancy>
              <provider></provider>
            </stakeholder>
          </risk>
          <validation>
              <plan></plan>
          </validation>
      </requirements>
      <provider>
        <risk assessment="HAZOP, FMEA, SWOT, PETSLE">
            <responsibility></responsibility>      
            <security></security>      
            <bcpdrp></bcpdrp>
        </risk>
        <contracts type="traditional, offTheShelfSolution, management"></contracts>
        <finance type="priceBased, costBased, targetBased"></finance>
      </provider>
    </decision> 
    <execution>
      <review>
        <timing></timing>
        <budget></budget>
        <value>
          <validation>
            <report>
                <estimation></estimation>
            </report>
          </validation>
          <change>
            <functional></functional>
            <system></system>
            <stakeholder></stakeholder>
          </change>
        </value>
      </review>
    </execution>
    <deployment>
      <finance></finance>
      <infrastructure></infrastructure>
      <integration></integration>
      <documentation></documentation>
    </deployment>
    <routine>
      <review>
        <maturity></maturity>
        <event></event>
        <stakeholder></stakeholder>
        <provider></provider>
      </review>
      <finance></finance>
      <infrastructure></infrastructure>
      <integration></integration>
      <event></event>
    </routine>
  </stragety>
</root>
